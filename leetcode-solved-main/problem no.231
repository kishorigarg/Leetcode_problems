class Solution {
public:
    bool isPowerOfTwo(int n) {
        
        for(int i=0;i<=30;i++){
            int ans =pow(2,i);
            if(ans==n){
                 return true;
            }
              
        }
       return false;
    }
};//approach simple h agr vo number pow 2 h to true then false ,30 
